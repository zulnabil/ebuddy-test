{
  "name": "backend-repo",
  "version": "1.0.0",
  "main": "dist/core/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r module-alias/register src/core/app.ts",
    "start": "node -r module-alias/register dist/core/app.js",
    "build": "tsc"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "firebase-admin": "^9.11.0",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^16.0.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  },
  "_moduleAliases": {
    "@config": "src/config",
    "@controller": "src/controller",
    "@core": "src/core",
    "@entities": "src/entities",
    "@middleware": "src/middleware",
    "@repository": "src/repository",
    "@routes": "src/routes"
  }
}
